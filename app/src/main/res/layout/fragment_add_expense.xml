<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="409dp"
    android:layout_height="227dp"
    android:background="#C5DDD9">

    <CheckBox
        android:id="@+id/amusementCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Amusement"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/householdCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Household"
        app:layout_constraintStart_toStartOf="@+id/amusementCheckBox"
        app:layout_constraintTop_toBottomOf="@+id/amusementCheckBox" />

    <CheckBox
        android:id="@+id/transportationCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Tranportation"
        app:layout_constraintStart_toStartOf="@+id/householdCheckBox"
        app:layout_constraintTop_toBottomOf="@+id/householdCheckBox" />

    <CheckBox
        android:id="@+id/diningCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="125dp"
        android:text="Dining"
        app:layout_constraintStart_toStartOf="@+id/amusementCheckBox"
        app:layout_constraintTop_toTopOf="@+id/amusementCheckBox" />

    <CheckBox
        android:id="@+id/hcWellCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Healthcare / Wellness"
        app:layout_constraintStart_toStartOf="@+id/diningCheckBox"
        app:layout_constraintTop_toBottomOf="@+id/diningCheckBox" />

    <CheckBox
        android:id="@+id/groceriesCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Groceries"
        app:layout_constraintStart_toStartOf="@+id/hcWellCheckBox"
        app:layout_constraintTop_toBottomOf="@+id/hcWellCheckBox" />

    <CheckBox
        android:id="@+id/otherCheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="125dp"
        android:text="Other"
        app:layout_constraintStart_toStartOf="@+id/diningCheckBox"
        app:layout_constraintTop_toTopOf="@+id/diningCheckBox" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="68dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="75dp"
        android:backgroundTint="#98C1D8"
        app:layout_constraintStart_toStartOf="@+id/otherCheckBox"
        app:layout_constraintTop_toBottomOf="@+id/otherCheckBox"
        app:srcCompat="@drawable/baseline_save_24" />
</androidx.constraintlayout.widget.ConstraintLayout>